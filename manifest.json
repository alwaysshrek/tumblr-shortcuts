{
    "name": "Extended keyboard navigation for Tumblr",
    "description": "Access Tumblr functions from keyboard",
    "version": "1.2",
    "icons": {
        "48": "48.png",
        "128": "128.png"
    },
    "permissions": [
        "http://*.tumblr.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://*.tumblr.com/*"],
            "js": ["content_script.js"]
        }
    ]
}
