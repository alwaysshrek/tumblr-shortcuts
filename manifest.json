{
    "name": "Extended keyboard navigation for Tumblr",
    "version": "1.0",
    "icons": {
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
        "http://*.tumblr.com/*"
    ],
    "content_scripts": [
        {
            "matches": ["http://*.tumblr.com/*"],
            "js": ["content_script.js"]
        }
    ]
}
